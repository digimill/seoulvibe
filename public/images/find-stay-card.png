
<svg width="600" height="400" viewBox="0 0 600 400" xmlns="http://www.w3.org/2000/svg">
  <rect width="100%" height="100%" fill="white"/>
  
  <!-- Map base -->
  <rect x="150" y="120" width="300" height="180" fill="none" stroke="#111111" stroke-width="2"/>
  <line x1="250" y1="120" x2="250" y2="300" stroke="#111111" stroke-width="1.5"/>
  <line x1="350" y1="120" x2="350" y2="300" stroke="#111111" stroke-width="1.5"/>

  <!-- Hotel building -->
  <rect x="260" y="180" width="80" height="80" fill="none" stroke="#111111" stroke-width="2"/>
  
  <!-- Windows -->
  <rect x="270" y="210" width="10" height="10" fill="none" stroke="#111111" stroke-width="1"/>
  <rect x="290" y="210" width="10" height="10" fill="none" stroke="#111111" stroke-width="1"/>
  <rect x="310" y="210" width="10" height="10" fill="none" stroke="#111111" stroke-width="1"/>
  <rect x="270" y="235" width="10" height="10" fill="none" stroke="#111111" stroke-width="1"/>
  <rect x="290" y="235" width="10" height="10" fill="none" stroke="#111111" stroke-width="1"/>
  <rect x="310" y="235" width="10" height="10" fill="none" stroke="#111111" stroke-width="1"/>

  <!-- Door (accent) -->
  <rect x="295" y="230" width="20" height="30" fill="none" stroke="#E4572E" stroke-width="3"/>

  <!-- Location pin -->
  <circle cx="420" cy="180" r="20" fill="none" stroke="#111111" stroke-width="2"/>
  <circle cx="420" cy="180" r="5" fill="#E4572E"/>
  <line x1="420" y1="200" x2="420" y2="240" stroke="#111111" stroke-width="2"/>
</svg>
